---
# Variables listed here are applicable to all host groups
wp_version: 4.3
wp_sha256sum: 3b0db3abe8504f15a33cf64188a493ec0de01eaa8d20e37c3d6a1d9fa0a40fb4

# MySQL settings
mysqlservice: mysqld
mysql_port: 3306

# These are the WordPress database settings
wp_db_name: wordpress
wp_db_user: wordpress
wp_db_password: MYPASS 

# This is used for the nginx server configuration, but access to the
# WordPress site is not restricted by a named host.
nginx_port: 80
server_hostname: MyHOST 

# Disable All Updates
# By default automatic updates are enabled, set this value to true to disable all automatic updates
auto_up_disable: false

#Define Core Update Level
# true  = Development, minor, and major updates are all enabled
# false = Development, minor, and major updates are all disabled
# minor = Minor updates are enabled, development, and major updates are disabled
core_update_level: true

#Use two factoe auth app credentials in order to use Gmail
gmail_user: Myuser
gmail_pass: MyPass

#s3 credentials for backups
s3_access_key: XXXXXXXXXXX 
s3_secret_key: XXXXXXXXXXXX
